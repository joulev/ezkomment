{
  "name": "ezkomment",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "cache-docs": "node client/scripts/cacheDocs.mjs",
    "dev-ezk-package": "mkdir -p public/v1/js && cp client/packages/ezkomment.js public/v1/js",
    "build-ezk-package": "mkdir -p public/v1/js && uglifyjs --compress --mangle --output public/v1/js/ezkomment.min.js -- client/packages/ezkomment.js",
    "dev": "yarn dev-ezk-package && yarn cache-docs && next dev",
    "prebuild": "yarn build-ezk-package && yarn cache-docs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "emulate:dev": "__EMULATE__=true firebase emulators:exec 'yarn dev' --project demo-proj --ui",
    "test:client": "jest --watch --coverage --config=jest.client.config.js",
    "test:client:ci": "jest --ci --coverage --config=jest.client.config.js",
    "emulate": "firebase emulators:start --project demo-proj",
    "test:server": "jest --watch --coverage --config=jest.server.config.js",
    "test:server:ci": "jest --ci --coverage --config=jest.server.config.js",
    "emulate:test:server": "firebase emulators:exec 'yarn test:server' --project demo-proj",
    "emulate:test:server:ci": "firebase emulators:exec 'yarn test:server:ci' --project demo-proj"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mdx-js/loader": "^2.1.3",
    "@mdx-js/react": "^2.1.3",
    "@monaco-editor/react": "^4.4.5",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@victorioberra/trianglify-browser": "^4.1.1",
    "capture-website": "^3.1.0",
    "clsx": "^1.2.1",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "escape-string-regexp": "^5.0.0",
    "firebase": "^9.10.0",
    "firebase-admin": "^11.0.1",
    "framer-motion": "^7.3.6",
    "js-confetti": "^0.10.2",
    "jsdom": "^20.0.0",
    "json5": "^2.2.1",
    "merge-ranges": "^1.0.2",
    "monaco-editor": "^0.34.0",
    "multer": "^1.4.5-lts.1",
    "next": "^12.3.0",
    "next-connect": "^0.13.0",
    "next-seo": "^5.5.0",
    "nprogress": "^0.2.0",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-full-screen": "^1.1.1",
    "react-image": "^4.0.3",
    "react-intersection-observer": "^9.4.0",
    "react-string-replace": "^1.1.0",
    "rehype-external-links": "^2.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^10.1.0",
    "remark-toc": "^8.0.1",
    "strip-markdown": "^5.0.0",
    "swr": "^1.3.0",
    "typed.js": "^2.0.12",
    "unified": "^10.1.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
