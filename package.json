{
  "name": "ezkomment",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "cache-docs": "node client/scripts/cacheDocs.mjs",
    "dev-ezk-package": "mkdir -p public/v1/js && cp client/packages/ezkomment.js public/v1/js",
    "build-ezk-package": "mkdir -p public/v1/js && uglifyjs --compress --mangle --output public/v1/js/ezkomment.min.js -- client/packages/ezkomment.js",
    "dev": "pnpm dev-ezk-package && pnpm cache-docs && next dev",
    "prebuild": "pnpm build-ezk-package && pnpm cache-docs",
    "build": "pnpm prebuild && next build",
    "start": "next start",
    "lint": "next lint",
    "emulate:dev": "__EMULATE__=true firebase emulators:exec 'pnpm dev' --project demo-proj --ui",
    "test:client": "jest --watch --coverage --config=jest.client.config.js",
    "test:client:ci": "jest --ci --coverage --config=jest.client.config.js",
    "emulate": "firebase emulators:start --project demo-proj",
    "test:server": "jest --watch --coverage --config=jest.server.config.js",
    "test:server:ci": "jest --ci --coverage --config=jest.server.config.js",
    "emulate:test:server": "firebase emulators:exec 'pnpm test:server' --project demo-proj",
    "emulate:test:server:ci": "firebase emulators:exec 'pnpm test:server:ci' --project demo-proj"
  },
  "dependencies": {
    "@vercel/og": "0.0.20",
    "client-only": "0.0.1",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "firebase": "9.13.0",
    "firebase-admin": "11.2.0",
    "framer-motion": "7.6.4",
    "gray-matter": "4.0.3",
    "json5": "2.2.1",
    "lucide-react": "0.98.0",
    "merge-ranges": "1.0.2",
    "next": "canary",
    "next-connect": "next",
    "next-mdx-remote": "4.1.0",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.4.0",
    "react-string-replace": "1.1.0",
    "rehype-external-links": "2.0.1",
    "rehype-raw": "6.1.1",
    "rehype-sanitize": "5.0.1",
    "rehype-slug": "5.1.0",
    "rehype-stringify": "9.0.3",
    "remark": "14.0.2",
    "remark-gfm": "3.0.1",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "remark-toc": "8.0.1",
    "server-only": "0.0.1",
    "strip-markdown": "5.0.0",
    "swr": "1.3.0",
    "typed.js": "2.0.12",
    "unified": "10.1.2",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/merge-ranges": "1.0.0",
    "@types/node": "18.11.9",
    "@types/nprogress": "0.2.0",
    "@types/prettier": "2.7.1",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/validator": "13.7.10",
    "autoprefixer": "10.4.13",
    "eslint": "8.27.0",
    "eslint-config-next": "canary",
    "firebase-tools": "11.16.0",
    "jest": "29.2.2",
    "jest-environment-jsdom": "29.2.2",
    "jest-environment-node": "29.2.2",
    "next-router-mock": "0.7.4",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "tailwindcss": "3.2.2",
    "typescript": "4.8.4",
    "uglify-js": "3.17.4"
  }
}
